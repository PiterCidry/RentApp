<Window x:Class="RentAppMVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RentAppMVVM"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Margin="92,10,0,0" VerticalAlignment="Top" Height="166" Width="337" ItemsSource="{Binding Nieruchomosc}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Ulica" Binding="{Binding Ulica}" MinWidth="200"></DataGridTextColumn>
                <DataGridTextColumn Header="Miasto" Binding="{Binding Miasto}" MinWidth="200"></DataGridTextColumn>
        </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
